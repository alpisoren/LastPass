        <section id="main-content">
            <section class="wrapper">
                <div class="row">
                    <div class="col-lg-12">
                        <h3 class="page-header"><i class="fa fa-laptop"></i> Admin Paneli</h3>
                        <ol class="breadcrumb">
                            <li><i class="fa fa-home"></i><a href="#">@ControllerAdi</a></li>
                            @*<li><i class="fa fa-laptop"></i>Dashboard</li>*@
                        </ol>
                    </div>
                </div>

                @RenderBody()
